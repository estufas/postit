<% if @vote.valid? %>
$("#comment_<%= @comment.id %>_votes").html("<%= @comment.total_votes %> votes")
$("#comment_<%= @comment.id %>up_votes").html("<%= @comment.up_votes %>")
$("#comment_<%= @comment.id %>down_votes").html("<%= @comment.down_votes %>")
<% else %>
  alert('You can only vote on a post once.');
<% end %>


